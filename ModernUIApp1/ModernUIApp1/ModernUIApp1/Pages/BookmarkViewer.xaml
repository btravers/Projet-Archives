<UserControl x:Class="ModernUIApp1.Pages.BookmarkViewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    
    <Grid Style="{StaticResource ContentRoot}">
            <DockPanel>
                <StackPanel Orientation="Horizontal" DockPanel.Dock="Top" Margin="150,-150,10,10">
                    <mui:ModernFrame Source="/Content/View/Common/Bookmark/BookmarkToolbar.xaml"/>
                </StackPanel>

                <mui:ModernFrame Source="/Content/View/Common/Bookmark/BookmarkResult.xaml"/>
            </DockPanel>
    </Grid>
</UserControl>
